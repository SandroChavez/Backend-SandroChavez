<main>
    <h1>Your Cart</h1>
    <section>
        <ul>
            {{#each products}}
            <li>
                <p>Tittle: {{this.product.tittle}}</p>
                <p>price: {{this.product.price}}</p>
                <p>quantity: {{quantity}}</p>
            </li>
            {{/each}}
        </ul>
    </section>
</main>